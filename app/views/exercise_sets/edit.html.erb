<%= turbo_frame_tag dom_id(@set, :edit), class: "flex contents" do %>
  <%= form_with model: @set, url: exercise_exercise_set_path(exercise_id: @set.exercise.id, set_id: @set.id) do |form| %>
    <%= @index %>
    <%= form.number_field :intensity, in: 0..10 %>
    <%= form.number_field :weight %>
    <%= form.number_field :reps %>
    <%= form.submit %>
  <% end %>
<% end %>
